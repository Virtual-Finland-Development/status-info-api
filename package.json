    "@types/jsonwebtoken": "^9.0.1",
    "@types/uuid": "^9.0.0",
    "eslint": "^8.32.0",
    "nodemon": "^2.0.20",
    "prettier": "^2.8.3",
    "pulumi": "^0.0.1",
    "rimraf": "^4.1.1",
